<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="onsc_docket_state_square_view_tree" model="ir.ui.view">
        <field name="name">onsc.docket.state.square.tree</field>
        <field name="model">onsc.docket.state.square</field>
        <field name="arch" type="xml">
            <tree string="Estados de las plazas" editable="top">
                <field name="code"/>
                <field name="name"/>
                <field name="active" invisible="1"/>
            </tree>
        </field>
    </record>
    <record id="onsc_docket_state_square_view_form" model="ir.ui.view">
        <field name="name">onsc.docket.state.square.form</field>
        <field name="model">onsc.docket.state.square</field>
        <field name="arch" type="xml">
            <form string="Estados de las plazas">
                <sheet>
                    <group>
                        <group>
                            <field name="code"/>
                            <field name="name"/>
                            <field name="active" invisible="1"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="onsc_docket_state_square_view_search" model="ir.ui.view">
        <field name="name">onsc.docket.state.square.search</field>
        <field name="model">onsc.docket.state.square</field>
        <field name="arch" type="xml">
            <search string="Estados de las plazas">
                <field name="code"/>
                <field name="name"/>
            </search>
        </field>
    </record>
    <record id="onsc_docket_state_square_action" model="ir.actions.act_window">
        <field name="name">Estados de la plazas</field>
        <field name="res_model">onsc.docket.state.square</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Crear un nuevo estado de la plaza
            </p>
        </field>
    </record>
</odoo>