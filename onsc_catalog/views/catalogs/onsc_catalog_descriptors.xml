<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="onsc_catalog_descriptor1" model="ir.actions.act_window">
            <field name="name">Descriptor1</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">onsc.catalog.descriptor1</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Crear una nueva Descriptor 1
                </p>
            </field>
        </record>

        <record id="onsc_catalog_descriptor1_form" model="ir.ui.view">
            <field name="name">onsc.catalog.descriptor1.form</field>
            <field name="model">onsc.catalog.descriptor1</field>
            <field name="arch" type="xml">
                <form string="Descriptor 1">
                    <header/>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                            <field name="description"/>
                            <field name="occupational_family_id"/>
                            <field name="active" invisible="1"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="onsc_catalog_descriptor1_tree" model="ir.ui.view">
            <field name="name">onsc.catalog.descriptor1.tree</field>
            <field name="model">onsc.catalog.descriptor1</field>
            <field name="arch" type="xml">
                <tree string="Descriptor 1">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="description"/>
                    <field name="occupational_family_id"/>
                </tree>
            </field>
        </record>



