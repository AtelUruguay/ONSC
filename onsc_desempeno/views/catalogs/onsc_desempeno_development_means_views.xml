<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
     <record id="onsc_desempeno_development_means_tree" model="ir.ui.view">
        <field name="name">onsc.desempeno.development_means.tree</field>
        <field name="model">onsc.desempeno.development_means</field>
        <field name="arch" type="xml">
            <tree string="Medios de desarrollo">
                <field name="name"/>
                <field name="description"/>
                <field name="active" invisible="1"/>
            </tree>
        </field>
    </record>

    <record id="onsc_desempeno_development_means_form" model="ir.ui.view">
        <field name="name">onsc.desempeno.development_means.form</field>
        <field name="model">onsc.desempeno.development_means</field>
        <field name="arch" type="xml">
            <form string="Medios de desarrollo">
                <header/>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="description"/>
                            <field name="active" invisible="1"/>
                        </group>
                        <group/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id='onsc_desempeno_development_means_action' model='ir.actions.act_window'>
        <field name="name">Medios de desarrollo</field>
        <field name="res_model">onsc.desempeno.development_means</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Crear una nueva dimensi√≥n
            </p>
        </field>
    </record>
</odoo>
